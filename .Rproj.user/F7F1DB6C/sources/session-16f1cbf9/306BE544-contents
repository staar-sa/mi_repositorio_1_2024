##TIO BEN##
umbral <- 93
spider_man <- 85
wonder_woman <- 90
capitana_marvel <- 95
batman <- 80
hulk <- 75
thor <- 92
iron_man <- 88
superman <- 94
flash <- 79
aquaman <- 82

frz <- function(n) {
  if (n > umbral) {
    return("un gran poder conlleva una gran resppnsabilidad")
  } else {
    return("ni lo intentes")
  } 
}

frz (flash)
frz(superman)

if (spider_man > umbral) {
  print("un gran poder conlleva una gran responsablidad")
} else if (spider_man < umbral) {
  print("ni lo intentes, regresate a la escuela, eres chafa")
} 

